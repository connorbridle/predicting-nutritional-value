<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ExampleFoodDB">
   <children>
      <TableView fx:id="table" layoutX="90.0" layoutY="54.0" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="calsColumn" prefWidth="59.0" text="Calories" />
            <TableColumn fx:id="fatColumn" prefWidth="59.0" text="Fat" />
            <TableColumn fx:id="satFatColumn" prefWidth="59.0" text="Sat-Fat" />
            <TableColumn fx:id="carbsColumn" prefWidth="59.0" text="Carbs" />
            <TableColumn fx:id="sugarsColumn" prefWidth="59.0" text="Sugars" />
            <TableColumn fx:id="fibreColumn" prefWidth="59.0" text="Fibre" />
            <TableColumn fx:id="proteinColumn" prefWidth="59.0" text="Protein" />
            <TableColumn fx:id="saltColumn" prefWidth="59.0" text="Salt" />
            <TableColumn fx:id="buttonColumn" prefWidth="59.0" text="Load" />
        </columns>
      </TableView>
      <Button fx:id="SubmitFoodButton" layoutX="317.0" layoutY="510.0" mnemonicParsing="false" onAction="#submitFoodItemButton" text="Load selected Food Item" />
      <Text fill="#ee2c2c" layoutX="301.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Food Database">
         <font>
            <Font size="27.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
