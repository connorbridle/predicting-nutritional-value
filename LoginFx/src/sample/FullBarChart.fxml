<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefWidth="900.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FullBarChartController">
    <top>
        <HBox prefHeight="50.0" prefWidth="900.0" style="-fx-background-color: #1e90ff#1e90ff #1e90ff#1e90ff;" BorderPane.alignment="CENTER" />
    </top>
    <bottom>
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="900.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#returnToOutput" text="Return to results" />
            </children>
        </HBox>
    </bottom>
    <center>
        <VBox fx:id="myBox" alignment="CENTER" prefHeight="500.0" prefWidth="900.0">
            <BorderPane.margin>
                <Insets bottom="10.0" left="150.0" right="150.0" top="10.0" />
            </BorderPane.margin>
        </VBox>
    </center>
</BorderPane>
