<BorderPane fx:id="myPane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RedResultController">
   <center>
      <AnchorPane maxHeight="500.0" maxWidth="800.0" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Circle fill="#f83b22" layoutX="406.0" layoutY="286.0" radius="100.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
            <Label layoutX="350.0" layoutY="406.0" text="Inadvisable" underline="true" wrapText="true">
               <font>
                  <Font name="Arial Italic" size="23.0" />
               </font>
            </Label>
            <Label layoutX="330.0" layoutY="106.0" text="Program Decision">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="250.0" layoutY="65.0" prefHeight="53.0" prefWidth="314.0" text="Should I Eat This?" textFill="#b22626">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #1e90ff#1e90ff;" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <JFXButton fx:id="continueButton" onAction="#continueToResults" style="-fx-background-color: FF795C#FF795C;" text="Continue" BorderPane.alignment="CENTER" />
