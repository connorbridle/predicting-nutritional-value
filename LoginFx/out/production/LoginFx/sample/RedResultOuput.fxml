<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="myPane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RedResultController">
   <center>
      <AnchorPane prefWidth="900.0" BorderPane.alignment="CENTER">
         <children>
            <Circle fill="#f83b22" layoutX="458.0" layoutY="234.0" radius="100.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
            <Label layoutX="396.0" layoutY="356.0" text="Inadvisable" underline="true" wrapText="true">
               <font>
                  <Font name="Arial Italic" size="23.0" />
               </font>
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
            </Label>
            <Label layoutX="382.0" layoutY="84.0" text="Program Decision">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="315.0" layoutY="16.0" prefHeight="53.0" prefWidth="314.0" text="Should I Eat This?" textFill="#b22626">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #1e90ff#1e90ff;" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="900.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="continueButton" mnemonicParsing="false" onAction="#continueToResults" text="Continue To Results" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
