<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="myPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AmberResultController">
   <center>
      <AnchorPane maxHeight="500.0" maxWidth="800.0" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Circle fill="#f7aa23" layoutX="406.0" layoutY="286.0" radius="100.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
            <Label layoutX="274.0" layoutY="406.0" text="Needs further consideration" underline="true" wrapText="true">
               <font>
                  <Font name="Arial Italic" size="23.0" />
               </font>
            </Label>
            <Label layoutX="330.0" layoutY="106.0" text="Program Decision">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="250.0" layoutY="65.0" prefHeight="53.0" prefWidth="314.0" text="Should I Eat This?" textFill="#b22626">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="900.0" style="-fx-background-color: #1e90ff#1e90ff;" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#returnToStart" text="Restart">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin>
            </Button>
         </children></HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="900.0" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="continueButton" onAction="#continueToResults" style="-fx-background-color: FF795C#FF795C;" text="Continue" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
